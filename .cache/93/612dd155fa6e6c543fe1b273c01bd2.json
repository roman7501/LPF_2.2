ea:focus,\r\nbutton:focus {\r\n  outline: none;\r\n}\r\ninput[type='submit']:hover,\r\nbutton:hover {\r\n  cursor: pointer;\r\n}\r\ninput::-ms-clear {\r\n  display: none; /*─ IE11 → remove cross to close ─*/\r\n}\r\ntextarea {\r\n  overflow: auto; /*─ Hide scroll bar on IE ─*/\r\n}\r\na {\r\n  display: block;\r\n  color: inherit;\r\n  text-decoration: none;\r\n}\r\nimg, video, svg, picture {\r\n  display: block;\r\n}\r\nimg, video {\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n// svg {\r\n//   width: 100%;\r\n//   height: 100%;\r\n// }\r\niframe {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n","src\\scss\\base.scss":"body {\r\n  font-family: itc-avant-garde-gothic-pro, sans-serif;\r\n  color: #fff;\r\n  overflow: hidden;\r\n}\r\n\r\ncanvas {\r\n  display: block;\r\n}\r\n\r\n#container {\r\n  width: 100vw;\r\n  height: 100vh;\r\n}\r\n","src\\scss\\home.scss":".main {\r\n  position: absolute;\r\n  top: 200px;\r\n  right: 200px;\r\n  bottom: 0;\r\n  display: flex;\r\n  flex-direction: column;\r\n  overflow: auto;\r\n}\r\n\r\n.time__wrapper {\r\n  position: relative;\r\n  display: flex;\r\n  align-items: center;\r\n  margin-bottom: 24px;\r\n  font-size: 24px;\r\n  padding: 20px;\r\n  cursor: pointer;\r\n  opacity: 0.25;\r\n  transition: 0.25s;\r\n\r\n  &:hover {\r\n    opacity: 1;\r\n    #placeholder {\r\n      transform: translateY(100%);\r\n      opacity: 0;\r\n    }\r\n    #play {\r\n      transform: translateY(0);\r\n      opacity: 1;\r\n    }\r\n  }\r\n}\r\n\r\n.hour {\r\n  margin-right: 64px;\r\n}\r\n\r\n.info {\r\n  position: relative;\r\n}\r\n\r\n#placeholder {\r\n  transition: 0.3s;\r\n}\r\n\r\n#play {\r\n  opacity: 0;\r\n  position: absolute;\r\n  bottom: 0;\r\n  transform: translateY(-100%);\r\n  transition: 0.3s;\r\n}\r\n\r\n.date-play {\r\n  font-size: 18px;\r\n  color: gray !important;\r\n}\r\n\r\n/* Others */\r\n\r\n.join {\r\n  position: fixed;\r\n  bottom: 70px;\r\n  right: 80px;\r\n  text-transform: uppercase;\r\n  letter-spacing: 1px;\r\n  line-height: 1.5;\r\n  transition: 0.3s;\r\n\r\n  &:before {\r\n    content: \"\";\r\n    position: absolute;\r\n    bottom: 0;\r\n    right: 0;\r\n    width: 100%;\r\n    height: 2px;\r\n    background-color: #fff;\r\n    transform-origin: center left;\r\n    transform: scaleX(0);\r\n    transition: 0.4s ease-out;\r\n  }\r\n  &:hover:before {\r\n    transform: scaleX(1);\r\n    transform-origin: center right;\r\n    transition: 0.3s;\r\n  }\r\n  &:active {\r\n    transform: scale(0.95);\r\n    opacity: 0.8;\r\n  }\r\n}\r\n\r\n.title {\r\n  position: fixed;\r\n  top: 80px;\r\n  left: 40px;\r\n  font-size: 24px;\r\n  opacity: 0.6;\r\n}\r\n\r\n.date {\r\n  position: fixed;\r\n  bottom: 80px;\r\n  left: 40px;\r\n\r\n  p {\r\n    transition: 0.5s;\r\n    transition-delay: 0.2s;\r\n  }\r\n\r\n  p:first-child {\r\n    margin-bottom: 12px;\r\n  }\r\n}\r\n\r\n@media (max-width: 1050px) {\r\n  .main {\r\n    right: 100px;\r\n  }\r\n}\r\n\r\n@media (max-width: 750px) {\r\n  .title {\r\n    top: 15px;\r\n    left: 5px;\r\n    font-size: 18px;\r\n  }\r\n  .join {\r\n    bottom: unset;\r\n    top: 70px;\r\n    right: 10px;\r\n  }\r\n\r\n  .date {\r\n    display: none;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"1898a132d0cd4dce923d70d825bd5e1f","cacheData":{"env":{}}}